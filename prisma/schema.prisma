generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Product {
  name        String
  id          Int     @id @default(autoincrement())
  price       Float
  codeProduct String?
  description String
  imgBase64   String  @db.LongText
  category    String? @db.VarChar(255)
}
